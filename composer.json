{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-imap": "^7.3",
        "ext-ldap": "^7.3",
        "barryvdh/laravel-ide-helper": "^2.6",
        "barryvdh/laravel-snappy": "^0.4.4",
        "bugsnag/bugsnag-laravel": "^2.15",
        "doctrine/dbal": "^2.9",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "fedeisas/laravel-mail-css-inliner": "^2.2",
        "fennb/phirehose": "^1.0",
        "fideloper/proxy": "~4.0",
        "google/recaptcha": "^1.2",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "dev-master",
        "jdavidbakr/mail-tracker": "^2.1",
        "jenssegers/agent": "^2.6",
        "knplabs/knp-snappy": "^1.1",
        "kordy/ticketit": "^0.4.5",
        "kouz/laravel-airbrake": "^0.4.1",
        "laravel/cashier": "^9.3",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "socialiteproviders/linkedin": "^3.0",
        "stripe/stripe-php": "^6.34",
        "zizaco/entrust": "^1.9"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
