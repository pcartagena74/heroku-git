{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "~7.1.0",
        "ext-imap": "*",
        "ext-ldap": "*",
        "barryvdh/laravel-ide-helper": "^2.4",
        "barryvdh/laravel-snappy": "^0.4.1",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^2.6",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "fennb/phirehose": "^1.0",
        "fideloper/proxy": "~3.0",
        "google/recaptcha": "~1.1",
        "graham-campbell/flysystem": "^4.1",
        "guzzlehttp/guzzle": "^6.2",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "jdavidbakr/mail-tracker": "~2.1",
        "jenssegers/agent": "^2.5",
        "knplabs/knp-snappy": "^1.0",
        "kordy/ticketit": "0.*",
        "kouz/laravel-airbrake": "^0.2.0",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.5",
        "laravel/lumen-framework": "5.5",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.5.*",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "limesurvey/limesurvey": "^3.2",
        "maatwebsite/excel": "^2.1",
        "msclics/laravel-phantomjs": "^1.0",
        "nicolaslopezj/searchable": "1.*",
        "plank/laravel-mediable": "^2.5",
        "simplesoftwareio/simple-qrcode": "^1.5",
        "socialiteproviders/linkedin": "dev-master",
        "spatie/laravel-activitylog": "dev-master",
        "spatie/laravel-link-checker": "^2.0",
        "spatie/laravel-referer": "^1.2",
        "stripe/stripe-php": "^4.13",
        "true/punycode": "dev-master",
        "zizaco/entrust": "dev-laravel-5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "tightenco/mailthief": "^0.3.7",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "vendor/fennb/phirehose/lib"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helper_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
